{
	"manifest_version": 2,
	"name": "Debugger",
	"short_name": "Debugger",
	"version": "0.2",
	"description": "Simple debugger that uses information sent via headers.",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"<all_urls>"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"devtools_page": "devtools.html"
}
