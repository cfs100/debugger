{
	"manifest_version": 2,
	"name": "Debugger",
	"short_name": "Debugger",
	"version": "0.1",
	"description": "Simple debugger that uses information sent via headers.",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"<all_urls>"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"js": ["debugger.js"],
		"matches": ["<all_urls>"]
	}]
}
